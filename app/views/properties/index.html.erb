<p id="notice"><%= notice %></p>

<div class="pull-right">
  <%= link_to 'New Property', new_property_path, class: 'btn btn-default' %>
</div>

<h1>Properties</h1>

<% @properties.each do |property| %>
  <h3><%= link_to property.title, property %></h3>
  <h4><%= property.property_type.name %></h4>
  <p><%= property.description %></p>
<% end %>

<%= will_paginate @properties %>
